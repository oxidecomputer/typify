{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TestNotPattern",
  "type": "object",
  "properties": {
    "key": {
      "type": "string",
      "description": "A unique identifier that must not be a UUID",
      "not": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
      }
    },
    "name": {
      "type": "string",
      "description": "A name that cannot be 'test'",
      "not": {
        "pattern": "^test$"
      }
    }
  },
  "required": ["key", "name"]
}
